config wifi-device 'radio0'
        option type 'mac80211'
        option channel '11'
        option path 'platform/ar934x_wmac'
        option htmode 'HT40-'
        option hwmode '802.11bgn'

config wifi-iface
        option device 'radio0'
        option mode 'sta'
        option ssid 'Cocacola :D'
        option encryption 'psk2'
        option key '12345678'
        option network 'wan'

config wifi-iface
        option device 'radio0'
        option mode 'ap'
        option ssid 'minhaz+'
        option encryption 'psk2'
        option key '87654321'
        option network 'lan'

